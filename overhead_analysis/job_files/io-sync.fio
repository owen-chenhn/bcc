[global]
filename=/dev/sdb
ioengine=sync
direct=1
runtime=60
ramp_time=30
time_based=1

[sync-read-04K] 
rw=read
bs=4k

[sync-read-64K]
rw=read
bs=64k

[sync-write-04K]
rw=write
bs=4k

[sync-write-64K]
rw=write
bs=64k

[sync-randread-04K]
rw=randread
bs=4k

[sync-randread-64K]
rw=randread
bs=64k

[sync-randwrite-04K]
rw=randwrite
bs=4k

[sync-randwrite-64K]
rw=randwrite
bs=64k
