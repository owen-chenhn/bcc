[global]
filename=/dev/sdb
buffered=1
runtime=60
ramp_time=30
time_based=1

[buffered-sync-read] 
ioengine=sync
rw=read 

[buffered-sync-write] 
ioengine=sync
rw=write 

[buffered-sync-randread] 
ioengine=sync
rw=randread 

[buffered-sync-randwrite] 
ioengine=sync
rw=randwrite 

[buffered-libaio-read] 
ioengine=libaio
rw=read 

[buffered-libaio-write] 
ioengine=libaio
rw=write 

[buffered-libaio-randread] 
ioengine=libaio
rw=randread 

[buffered-libaio-randwrite] 
ioengine=libaio
rw=randwrite 
