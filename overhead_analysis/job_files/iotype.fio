[global]
filename=/dev/sdb
runtime=60
time_based=1

[buffered-libaio-rw] 
ioengine=libaio
rw=rw 

[buffered-libaio-randrw]
ioengine=libaio
rw=randrw 

[buffered-sync-rw] 
ioengine=sync
rw=rw 

[buffered-sync-randrw] 
ioengine=sync
rw=randrw 

[direct-libaio-rw] 
direct=1
ioengine=libaio
rw=rw 

[direct-libaio-randrw] 
direct=1
ioengine=libaio
rw=randrw 

[direct-sync-rw] 
direct=1
ioengine=sync
rw=rw 

[direct-sync-randrw] 
direct=1
ioengine=sync
rw=randrw 
