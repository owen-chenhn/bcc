[global]
filename=/dev/sdb
ioengine=libaio
direct=1
runtime=60
ramp_time=30
time_based=1

[libaio-read-d16]
rw=read
iodepth=16

[libaio-read-d64]
rw=read
iodepth=64

[libaio-write-d16]
rw=write
iodepth=16

[libaio-write-d64]
rw=write
iodepth=64

[libaio-randread-d16]
rw=randread
iodepth=16

[libaio-randread-d64]
rw=randread
iodepth=64

[libaio-randwrite-d16]
rw=randwrite
iodepth=16

[libaio-randwrite-d64]
rw=randwrite
iodepth=64
