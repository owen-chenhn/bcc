[global]
filename=/dev/sdb
direct=1
runtime=60
time_based=1

[libaio-rw-d01]
ioengine=libaio
rw=rw
iodepth=1

[libaio-rw-d04]
ioengine=libaio
rw=rw
iodepth=4

[libaio-rw-d16]
ioengine=libaio
rw=rw
iodepth=16

[libaio-rw-d64]
ioengine=libaio
rw=rw
iodepth=64

[libaio-randrw-d01]
ioengine=libaio
rw=randrw
iodepth=1

[libaio-randrw-d04]
ioengine=libaio
rw=randrw
iodepth=4

[libaio-randrw-d16]
ioengine=libaio
rw=randrw
iodepth=16

[libaio-randrw-d64]
ioengine=libaio
rw=randrw
iodepth=64
